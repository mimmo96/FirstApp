<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.GreenApp.Graphic.MainActivity"
    android:background="#eeeeee">

    <include
        layout="@layout/graphic_content_main"
        android:layout_width="match_parent"
        android:layout_height="423dp"
        android:layout_below="@+id/editTextDateEnd"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="4dp" />

    <EditText
        android:id="@+id/editTextDatestart"
        android:layout_width="151dp"
        android:layout_height="39dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="7dp"
        android:layout_toStartOf="@+id/buttonStart"
        android:layout_toEndOf="@+id/textView4"
        android:ems="10"
        android:hint="GG/MM/YYYY"
        android:inputType="date" />

    <EditText
        android:id="@+id/editTextDateEnd"
        android:layout_width="151dp"
        android:layout_height="39dp"
        android:layout_below="@+id/editTextDatestart"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="8dp"
        android:layout_toStartOf="@+id/buttonStart"
        android:layout_toEndOf="@+id/textView10"
        android:ems="10"
        android:hint="GG/MM/YYYY"
        android:inputType="date" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="74dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="11dp"
        android:text="Data Inizio:" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="68dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="53dp"
        android:text="Data fine:" />

    <Button
        android:id="@+id/buttonStart"
        android:layout_width="58dp"
        android:layout_height="37dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="19dp"
        android:onClick="visualizzaGrafici"
        android:text="VAI" />


</RelativeLayout>